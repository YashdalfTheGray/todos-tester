{
  "name": "todos-tester",
  "version": "1.0.0",
  "description": "Test package for the todos app using Puppeteer, Jest, Javascript and Docker",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run clean-build && tsc",
    "test": "jest",
    "start": "npm test",
    "clean-build": "npx rimraf build",
    "clean-artifacts": "npx rimraf artifacts/**/*",
    "clean": "npm run clean-build && npm run clean-artifacts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/todos-tester.git"
  },
  "author": "Yash Kulshrestha",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/todos-tester/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/todos-tester#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/faker": "^4.1.4",
    "@types/is-docker": "^1.1.0",
    "@types/jest": "^23.3.9",
    "@types/puppeteer": "^1.10.0",
    "dotenv": "^6.1.0",
    "faker": "^4.1.0",
    "firebase": "^5.5.8",
    "is-docker": "^1.1.0",
    "jest": "^23.6.0",
    "puppeteer": "^1.10.0",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1"
  }
}
